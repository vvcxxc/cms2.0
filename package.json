    "@vueuse/core": "9.13.0",
    "@yzfe/svgicon": "^1.2.2",
    "@yzfe/vue3-svgicon": "^1.0.2",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "d3": "^7.9.0",
    "d3-hierarchy": "^3.1.2",
    "dayjs": "^1.11.10",
    "element-plus": "2.6.3",
    "element-tree-line": "^0.2.1",
